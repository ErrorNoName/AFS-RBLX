{
  "name": "sriblox-modern",
  "version": "2.0.0",
  "description": "Modern ScriptBlox search interface with Roact, Rodux and Flipper",
  "main": "out/init.lua",
  "scripts": {
    "compile": "npx rbxtsc --verbose --type=model",
    "watch": "npx rbxtsc -w",
    "build": "rojo build default.project.json --output \"SriBloxModern.rbxm\"",
    "serve": "rojo serve default.project.json",
    "bundle": "npm run build && remodel run ci/bundle.lua public/dev.lua dev verbose"
  },
  "keywords": ["roblox", "scriptblox", "roact", "typescript"],
  "author": "SriBlox Team",
  "license": "MIT",
  "devDependencies": {
    "@rbxts/compiler-types": "^1.3.2-types.0",
    "@rbxts/types": "^1.0.571",
    "roblox-ts": "^1.2.7",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@rbxts/flipper": "^2.0.1",
    "@rbxts/roact": "^1.4.0-ts.2",
    "@rbxts/roact-hooked": "^1.2.4",
    "@rbxts/roact-rodux-hooked": "^1.0.4",
    "@rbxts/rodux": "^3.0.0-ts.3",
    "@rbxts/services": "^1.2.0"
  }
}
